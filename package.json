{
  "name": "desktop-task-app",
  "version": "0.1.0",
  "private": true,
  "main": "./build/electron/electron.js",
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@emotion/styled": "^11.8.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@fullcalendar/react": "^5.10.1",
    "@fullcalendar/timegrid": "^5.10.1",
    "@mui/material": "^5.5.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "autoprefixer": "^10.4.2",
    "chart.js": "^3.7.1",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron-builder-squirrel-windows": "^23.0.3",
    "postcss": "^8.4.7",
    "rc-time-picker": "^3.7.3",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "react-switch": "^6.0.0",
    "sqlite3": "^5.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "victory": "^36.3.0",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "build": {
    "extends": null,
    "appId": "com.micha.task.project.app",
    "productName": "Task Project",
    "copyright": "",
    "directories": {
      "output": "dist",
      "buildResources": ""
    },
    "files": [
      "build/**/*",
      "package.json",
      "node_modules"
    ],
    "extraResources": [],
    "win": {
      "target": "nsis",
      "icon": "./build/icon.ico"
    },
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "x64"
        ]
      }
    }
  },
  "scripts": {
    "compile": "tsc -p electron/tsconfig.json",
    "start": "npm run compile && concurrently \"cross-env BROWSER=none cross-env craco start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "craco build && npm run compile",
    "test": "craco test",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist:mac": "electron-builder -m",
    "dist:win": "electron-builder -w"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-beta.0",
    "@types/jquery": "^3.5.14",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "electron-devtools-installer": "^3.2.0"
  }
}
